{
  "version": 2,
  "alias": "probot-cc.now.sh",
  "env": {
    "WEBHOOK_SECRET": "@webhook_secret",
    "APP_ID": "@app_id",
    "PRIVATE_KEY": "@private_key",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "src/probot.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "src/probot.js"
    }
  ]
}
